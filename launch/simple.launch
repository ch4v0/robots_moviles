<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find robots_moviles)/worlds/small_maze.world"/> 
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>  
  </include>

  <!-- launch kobuki-->
  <param name="kobuki_with_laser" command="$(find xacro)/xacro.py '$(find robots_moviles)/urdf/kobuki_with_laser.urdf.xacro'" />

  <param name="intensity" type="bool" value="false"/>

   <arg name="urdf_file" default="$(find xacro)/xacro.py '$(find robots_moviles)/urdf/kobuki_with_laser.urdf.xacro'" />
<param name="robot_description" command="$(arg urdf_file)" />

  <node name="kobuki_with_laser" pkg="gazebo_ros" type="spawn_model" args="-param kobuki_with_laser intensity -urdf
  -x 3.5
  -y 0.5
  -z 0.2
  -yaw 0.0000
   -model robot" />

   <arg name="map_file" default="$(find robots_moviles)/small_maze.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
    <param name="publish_frequency" type="double" value="30.0" />
  </node>

<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <param name="controller_frequency" value="10.0"/>
    <rosparam file="$(find robots_moviles)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robots_moviles)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robots_moviles)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find robots_moviles)/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robots_moviles)/param/dwa_local_planner_params.yaml" command="load" />
    <remap from="cmd_vel" to="mobile_base/commands/velocity"/>
  </node>

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find robots_moviles)/small_maze.rviz"/>
  <node pkg="tf" type="static_transform_publisher" name="map_to_odom_link" args="3.5 0.5 0 0 0 0 /map /odom 100" />
  <!--node pkg="tf" type="static_transform_publisher" name="odom_to_footprint" args="0 0 0 0 0 0 /odom /base_footprint 100" />
  <node pkg="tf" type="static_transform_publisher" name="odom_to_base_link" args="0 0 0 0 0 0 /base_footprint /base_link 100" />
  <node pkg="tf" type="static_transform_publisher" name="base_link_to_hokuyo" args="0 0 0 0 0 0 /base_link /hokuyo 100" />
  <node pkg="tf" type="static_transform_publisher" name="left" args="0 0 0 0 0 0 /base_link /wheel_left_link 100" />
  <node pkg="tf" type="static_transform_publisher" name="right" args="0 0 0 0 0 0 /base_link /wheel_right_link 100" /-->
 

</launch> 